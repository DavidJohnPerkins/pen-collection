<%- include('../includes/head.ejs') %>
	<link rel="stylesheet" href="/css/main.css">
	<link rel="stylesheet" href="/css/collection-item.css">
	</head>

	<body>
		<%- include('../includes/navigation.ejs') %>
		<main class="centered">
			<h1><%= item.map_title %></h1>
			<hr>
			<div class="parent">
				<div class="child child__left">
					<div class="map__detail__image">
						<img src="<%= item.map_image %>" alt="<%= item.map_title %>">
					</div>
				</div>
				<div class="child child__right">
					<div class="map__detail__image">
						<img src="<%= item.map_image_rear %>" alt="<%= item.map_title %>">
					</div>
				</div>
				</div>
			</div>
			<h2><%= item.main_settlements %></h2>
			<p><%= item.publish_date %></p>
			<%- include('../includes/add-to-cart.ejs') %>
		</main>
		<%- include('../includes/end.ejs') %>